<h1>Users::Addresses#edit</h1>
<p>Find me in app/views/users/addresses/edit.html.erb</p>



<div class="container">
　<h2>配送先編集</h2>
  <table>
    <%= form_for(@address) do |f| %>
    <tr>
      <th>氏名</th>
      <td>
        <%= @address.family_name %>
        <%= f.text_field :family_name %>
      </td>
      <td>
        <%= @address.first_name %>
        <%= f.text_field :first_name %>
      </td>
    </tr>
    <tr>
      <th>フリガナ</th>
      <td>
        <%= @address.kana_family_name %>
        <%= f.text_field :kana_family_name %>
      </td>
      <td>
        <%= @address.kana_first_name %>
        <%= f.text_field :kana_first_name %>
      </td>
    </tr>
    <tr>
      <th>郵便番号</th>
      <td>
        <%= @address.post_number %>
        <%= f.text_field :post_number %>
      </td>
    </tr>
    <tr>
      <th>住所1</th>
      <td>
        <%= @address.address %>
        <%= f.text_field :address %>
      </td>
    </tr>
    <tr>
      <th>電話番号</th>
      <td>
        <%= @address.phone_number %>
        <%= f.text_field :phone_number %>
      </td>
    </tr>
    <tr>
      <%= f.submit '編集' %>
    </tr>
    <% end %>
  </table>
</div>
