<div class="container">
  <div class="row">
    <div class="col-xs-12">
    <h2>Users</h2>
      <div id="nav-search">
        <%= form_tag admins_users_path, :method => 'get' do %>
            <p class="text-right">
                <%= text_field_tag :search, params[:search] %>

                <%= submit_tag "検索", :name  => nil %>


            </p>
             <% end %>
       
      </div>
    </div>

    <div class="col-xs-12">
      <table class="table table-striped">
        <thead>
         <tr>
          <th>Name</th>
          <th>Phone_Number</th>
          <th>E-mail</th>
          <th></th>
         <tr>
        </thead>

        
        

        <% @user.each do |user| %>
        <tbody>
          <tr>
            <td><%= link_to admins_user_path(user.id) do %>
              <%= user.family_name %><%= user.first_name %>
                <% end %>
            </td>
            <td><%= user.phone_number %></td>

            <td><%= user.email %></td>
          </tr>
        </tbody>
        <% end %>
      </table>
     <%= paginate @user %>
    </div>
  </div>
</div>