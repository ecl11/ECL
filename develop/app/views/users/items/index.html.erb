  <div id="wrap">
    <main class="container">
      <div id="main" class="clearfix">
        <ul class="breadcrumb"><li><a href="users/items">商品一覧</a></li></ul>
        <div class="search">
                            <%= paginate @items %>
      </div>
      <div class="box">
        <div class="product col-sm-12">
                            <% @items.each do |item| %>
            <div class="col-sm-3"><div class="iiage"><%= link_to users_item_path(item.id), class: "thumbnail" do %><%= attachment_image_tag item, :jacket_image, :fill, 100, 100, format: 'jpeg', fallback:"no_image.jpg" %><% end %></div>
              <div class="name"><%= link_to users_item_path(item.id) do %><%= item.product_name %><% end %></div>
              <div class="artist_name"><% if item.artist.present? %><%= item.artist.artist_name %><% end %></div>
        <div class="price">(税込) <%= item.price %> 円</div>
        </div><% end %>

  </div>
  </div>
  </div>
  <!--end#mian-->
  </main>
  </div>
