<h1>Users::Addresses#edit</h1>
<p>Find me in app/views/users/addresses/edit.html.erb</p>



<div class="container">
　<h2>住所の編集</h2>
  <table>
    <% @address.each do |address| %>
    <%= form_for(address, url: users_addresses_path) do |f| %>
    <tr>
      <th>氏名</th>
      <td>
        <%= f.text_field :family_name %>
      </td>
      <td>
        <%= f.text_field :first_name %>
      </td>
    </tr>
    <tr>
      <th>フリガナ</th>
      <td>
        <%= f.text_field :kana_family_name %>
      </td>
      <td>
        <%= f.text_field :kana_first_name %>
      </td>
    </tr>
    <tr>
      <th>郵便番号</th>
      <td>
        <%= f.text_field :post_number %>
      </td>
    </tr>
    <tr>
      <th>住所</th>
      <td>
        <%= f.text_field :address %>
      </td>
    </tr>
    <tr>
      <th>電話番号</th>
      <td>
        <%= f.text_field :phone_number %>
      </td>
    </tr>
    <tr>
      <%= f.submit 'この住所を追加する' %>
    </tr>
    <% end %>
    <% end %>
  </table>
</div>
